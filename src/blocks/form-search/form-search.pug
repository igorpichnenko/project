mixin form-search({data})

  - const {largeTitle,start,end,quests,dropdown,btn} = data

  form.card-search
    .card-search__wrapper
      .card-search__wrap-title
        +title(largeTitle)
      .card-search__calendar.js-datepicker-container
        .card-search__datepicker
         +datepicker(start)
        .card-search__datepicker
         +datepicker(end)
      .card-search__dropdown
        .card-search__title
         +title(guests)
        +dropdown(dropdown)
    .card-search__button
      +button(btn) 
